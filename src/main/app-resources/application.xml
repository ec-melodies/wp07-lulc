<?xml version="1.0" encoding="us-ascii"?>
<application xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" id="my_application">
  <jobTemplates>
    <jobTemplate id="my_template">
      <streamingExecutable>/application/ini.sh</streamingExecutable>
      <defaultParameters>
        <parameter id="MMU" title="MMU" abstract="Set the Minimum Mapping Unit in hectares for your final Land Cover map" scope="runtime">5</parameter>
      </defaultParameters>
    </jobTemplate>
  </jobTemplates>
  <workflow id="LandUseLandCover" title="Land Use Land Cover workflow" abstract="LULC production">
    <workflowVersion>1.0</workflowVersion>
    <node id="my_node">
      <job id="my_template" />
      <sources>
        <source id="tiles" title="Tile(s)" abstract="List of tiles" refid="string:list" scope="runtime">tile1,tile2</source>
        <source id="years" title="Year(s)" abstract="List of years" refid="string:list" scope="runtime">year1,year2</source>        
      </sources>
      <parameters>
        <parameter id="MMU">5</parameter>
      </parameters>
    </node>
  </workflow>
</application>